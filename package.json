{
  "name": "server",
  "version": "1.0.0",
  "description": "server of crm",
  "main": "index.js",
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "concurrently \"node index.js\" \"npm run client-install\" \"npm run client-build\" \"npm run client-preview\"",
    "serve": "nodemon index.js",
    "client-install": "npm install --prefix client",
    "client-build" : "npm run build --prefix client",
    "client-preview": "npm run preview --prefix client",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run serve\" \"npm run client\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.21"
  }
}
